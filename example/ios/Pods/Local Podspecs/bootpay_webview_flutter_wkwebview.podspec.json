{
  "name": "bootpay_webview_flutter_wkwebview",
  "version": "0.0.1",
  "summary": "A WebView Plugin for Flutter (Bootpay Fork).",
  "description": "A Flutter plugin that provides a WebView widget for Bootpay.\nForked from webview_flutter_wkwebview for Korean payment environment.",
  "homepage": "https://github.com/bootpay/bootpay_webview_flutter_wkwebview",
  "license": {
    "type": "BSD",
    "file": "../LICENSE"
  },
  "authors": {
    "Bootpay": "bootpay.co.kr"
  },
  "source": {
    "http": "https://github.com/bootpay/bootpay_webview_flutter_wkwebview"
  },
  "documentation_url": "https://github.com/bootpay/bootpay_webview_flutter_wkwebview",
  "source_files": "bootpay_webview_flutter_wkwebview/Sources/bootpay_webview_flutter_wkwebview/**/*.swift",
  "ios": {
    "dependencies": {
      "Flutter": []
    }
  },
  "osx": {
    "dependencies": {
      "FlutterMacOS": []
    }
  },
  "platforms": {
    "ios": "13.0",
    "osx": "10.15"
  },
  "pod_target_xcconfig": {
    "DEFINES_MODULE": "YES"
  },
  "resource_bundles": {
    "bootpay_webview_flutter_wkwebview_privacy": [
      "bootpay_webview_flutter_wkwebview/Sources/bootpay_webview_flutter_wkwebview/Resources/PrivacyInfo.xcprivacy"
    ]
  },
  "xcconfig": {
    "LIBRARY_SEARCH_PATHS": "$(TOOLCHAIN_DIR)/usr/lib/swift/$(PLATFORM_NAME)/ $(SDKROOT)/usr/lib/swift",
    "LD_RUNPATH_SEARCH_PATHS": "/usr/lib/swift"
  },
  "swift_versions": "5.0",
  "swift_version": "5.0"
}
